rabbitmq_plugins:
  - rabbitmq_management
rabbitmq_ssl        : no
rabbitmq_users_definitions:
  - vhost: /
    user: archiver
    password: archiver
    tags:
    - administrator

csf_tcp_in: "22, 15672, 5672, 7000"
csf_tcp6_in: "22, 15672, 5672, 7000"

csf_tcp_out: "15672, 5672, 80, 443, 7000"
csf_tcp6_out: "15672, 5672, 80, 443, 7000"

s3_access_key: CHANGE ME
s3_secret_key: CHANGE ME
s3_bucket: CHANGE ME

figshare_client_id: CHANG ME
figshare_client_secret: CHANGE ME

callback_addresses:
  - "http://192.168.111.111:7000/callback"
